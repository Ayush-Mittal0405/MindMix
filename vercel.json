{
  "framework": null,
  "buildCommand": "npm run build",
  "builds": [
    { "src": "api/[...all].ts", "use": "@vercel/node" },
    { "src": "client/build/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/[...all].ts" },
    { "src": "^/static/(.*)$", "dest": "client/build/static/$1" },
    { "src": "^/(.*\\.(?:js|css|svg|png|jpg|jpeg|gif|ico|webp|txt|json|map))$", "dest": "client/build/$1" },
    { "src": "^(?!/api/).*$", "dest": "client/build/index.html" }
  ]
}


