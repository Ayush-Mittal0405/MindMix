{
  "framework": null,
  "installCommand": "npm run install-all",
  "buildCommand": "npm run build",
  "builds": [
    { "src": "api/[...all].ts", "use": "@vercel/node" },
    { "src": "client/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/[...all].ts" },
    { "src": "^/static/(.*)$", "dest": "/client/static/$1" },
    { "src": "^/(.*\\.(?:js|css|svg|png|jpg|jpeg|gif|ico|webp|txt|json|map))$", "dest": "/client/$1" },
    { "handle": "filesystem" },
    { "src": "^(?!/api/).*$", "dest": "/client/index.html" }
  ]
}


